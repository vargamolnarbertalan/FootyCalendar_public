<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('header.ejs') %>
        <title>Error | Footy Calendar</title>
</head>

<body>
    <div id="loadOverlay"></div>
    <div class='progress' id='PreLoaderBar'>
        <div class='indeterminate'></div>
    </div>
    <div class="container">
        <!--
        <div class="back-container">
            <button class="btn" onclick="history.back()">BACK</button>
        </div>
        -->
        <h1>An error occured.</h1>
        <div class="error-box non-table warning" id="error-box1">
            <%=ejsdata.message%>
        </div>

        <button class="btn small-btn non-table" id="gotit-btn" type="button">GOT IT</button>


    </div>

    <%- include('footer.ejs') %>

        <div id="result"><br></div>

</body>

<script>
    $(document).ready(function() {

        $("#gotit-btn").on("click", function() {
            location.href = '/'
        })
    })
</script>

</html>